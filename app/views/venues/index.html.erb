<p id="notice"><%= notice %></p>

<h1>Venues</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Location</th>
      <th>Image url</th>
      <th>Rows</th>
      <th>Seats per row</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venues.each do |venue| %>
      <tr>
        <td><%= venue.name %></td>
        <td><%= venue.location %></td>
        <td><%= venue.image_url %></td>
        <td><%= venue.rows %></td>
        <td><%= venue.seats_per_row %></td>
        <td><%= link_to 'Show', venue %></td>
      
      </tr>
    <% end %>
  </tbody>
</table>

<br>


